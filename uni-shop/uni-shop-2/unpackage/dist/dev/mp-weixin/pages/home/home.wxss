/* 水平间距 */
.search-box {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 2;
}
swiper {
  height: 330rpx;
}
swiper .swiper-item, swiper ._img {
  width: 100%;
  height: 100%;
}
.nav-list {
  display: flex;
  justify-content: space-around;
  margin: 15px 0;
}
.nav-list .nav-item ._img {
  width: 128rpx;
  height: 140rpx;
}
.floor-title ._img {
  width: 100%;
  height: 60rpx;
}
.product-list {
  display: flex;
  padding-left: 10rpx;
}
.product-list .right-box {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}

