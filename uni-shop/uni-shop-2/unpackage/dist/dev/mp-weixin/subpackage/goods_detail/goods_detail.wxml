<block wx:if="{{goodsInfo.goods_name}}"><view class="goods-container"><swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}" circular="{{true}}"><block wx:for="{{goodsInfo.pics}}" wx:for-item="item" wx:for-index="i" wx:key="i"><swiper-item><image src="{{item.pics_big_url}}" data-event-opts="{{[['tap',[['preview',[i]]]]]}}" bindtap="__e" class="_img"></image></swiper-item></block></swiper><view class="goods-info-box"><view class="price">{{"￥"+goodsInfo.goods_price}}</view><view class="content"><view class="name">{{goodsInfo.goods_name}}</view><view class="collect"><uni-icons vue-id="e6d3fe5c-1" type="star" size="16" color="gray" bind:__l="__l"></uni-icons><text>收藏</text></view></view><view class="freight"><text>快递免运费</text></view></view><rich-text nodes="{{goodsInfo.goods_introduce}}"></rich-text><view class="goods-nav"><uni-goods-nav vue-id="e6d3fe5c-2" options="{{options}}" fill="{{true}}" button-group="{{buttonGroup}}" data-event-opts="{{[['^click',[['onClick']]],['^buttonClick',[['buttonClick']]]]}}" bind:click="__e" bind:buttonClick="__e" bind:__l="__l"></uni-goods-nav></view></view></block>